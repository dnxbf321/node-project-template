  "scripts": {
    "reset": "node bin/copy2es5.js",
    "compile": "npm-run-all reset --parallel es5-bin es5-scripts es5-eslintrc",
    "es5-bin": "babel bin -d ../npx-es5/bin",
    "es5-scripts": "babel scripts -d ../npx-es5/scripts",
    "es5-eslintrc": "babel .eslintrc.js -o ../npx-es5/.eslintrc.js"
  },
  "scripts-example": {
    "dev": "npx -t pre,asset,babel-asset,image,serve-client -e development",
    "build-dev": "npx -t pre,asset,babel-asset,image,webpack,postcss -e development",
    "build-exp": "npx -t pre,asset,babel-asset,image,webpack,postcss -e experiment",
    "build": "npx -t pre,asset,babel-asset,image,webpack,postcss -e production",
    "serve-client": "npx serve-client -e development",
    "pre": "npx pre",
    "webpack-dev": "npx webpack -e development",
    "webpack-exp": "npx webpack -e experiment",
    "webpack": "npx webpack -e production",
    "postcss-dev": "npx postcss -e development",
    "postcss-exp": "npx postcss -e experiment",
    "postcss": "npx postcss -e production",
    "babel-asset-dev": "npx babel-asset -e development",
    "babel-asset-exp": "npx babel-asset -e experiment",
    "babel-asset": "npx babel-asset -e production",
    "asset": "npx asset",
    "image": "npx image",
    "pack": "npx pack",
    "upload": "npx upload",
    "clean": "npx clean"
  },
